		}


	style(type="text/css")
		.page {font-family: Myriad Pro, sans-serif;color: #333;background-color: #fff;position: relative;height: auto;overflow-x: hidden;}.box {font-size: 16px;}.header {height: 100vh;background: url("../images/background.png");background-repeat: no-repeat;background-position: center top;background-size: cover;}.header__wrapper {background-color: rgba(193,194,197,0.8);width: 100%;height: 84px;display: -ms-flexbox;display: flex;}.header__content {width: 1149px;margin: 0 auto;display: -ms-flexbox;display: flex;-ms-flex-align: center;align-items: center;}.header__hg-group {display: -ms-flexbox;display: flex;-ms-flex-preferred-size: 100px;flex-basis: 100px;-ms-flex-positive: 1;flex-grow: 1;}@media screen and (max-width: 840px) {.header__hg-group {display: none;}}@media screen and (max-width: 840px) {.header .header__hg-group_mobile {display: -ms-flexbox;display: flex;-ms-flex-pack: center;justify-content: center;position: absolute;width: 100%;top: 374px;background-color: rgba(193,194,197,0.8);}}.logo {display: -ms-flexbox;display: flex;margin-left: 5px;}.logo__image {width: 280px;}@media (min-width: 50px) and (max-width: 891px) {.logo__image {width: 225px;}}.main-menu {font-size: 1.2em;-ms-flex-positive: 3;flex-grow: 3;margin-left: 35px;}.main-menu__default {opacity: 0.3;}.main-menu__wrap {display: -ms-flexbox;display: flex;}.main-menu__item {padding-top: 13px;padding-bottom: 13px;}.main-menu__link {color: #414141;text-decoration: none;padding: 10px;white-space: nowrap;position: relative;}.main-menu__link_hover::after {position: absolute;content: '';transition: all 300ms ease-in-out;width: 100%;height: 2px;bottom: 3px;background-color: #333;left: 0;transform: scale(0);}.main-menu__link:hover::after {transform: scaleX(0.7);}@media screen and (max-width: 840px) {.main-menu {display: none;}}@media screen and (max-width: 840px) {.main-menu_mobile {list-style: none;text-decoration: none;font-size: 1.7em;-ms-flex-preferred-size: 500px;flex-basis: 500px;-ms-flex-positive: 3;flex-grow: 3;position: absolute;top: 84px;background-color: rgba(193,194,197,0.8);width: 100%;display: -ms-flexbox;display: flex;margin-left: 0px;}.main-menu_mobile .main-menu__wrap_mobile {display: -ms-flexbox;display: flex;list-style: none;-ms-flex-wrap: nowrap;flex-wrap: nowrap;-ms-flex-direction: column;flex-direction: column;-ms-flex-align: center;align-items: center;width: 100%;}.main-menu_mobile .main-menu__link_mobile {padding-top: 13px;padding-bottom: 13px;margin-bottom: 220px;padding-left: 50px;padding-right: 50px;}.main-menu_mobile .main-menu__item_mobile {margin-bottom: 5px;}}.burger {margin-left: auto;cursor: pointer;padding: 20px;}.burger__line {position: relative;}.burger__line, .burger__line::before, .burger__line::after {display: block;width: 20px;height: 3px;background-color: #212121;}.burger__line::before, .burger__line::after {position: absolute;content: '';}.burger__line::before {top: -8px;}.burger__line::after {top: 8px;}.burger__line_active {background-color: transparent;transition-duration: 0.3s;}.burger__line_active::before {transform: translateY(8px) rotate(45deg);transition-duration: 0.3s;}.burger__line_active::after {transform: translateY(-8px) rotate(-45deg);}@media screen and (min-width: 840px) {.burger {display: none;}}.search-button {display: -ms-flexbox;display: flex;}.search-button__btn-search {background: none;border: none;}.search-button__btn-search:hover {transform: rotate(360deg);transition-duration: 0.5s;}.search-button__btn-search:active {transform: rotate(-360deg) scale(1.3);transition-duration: 1s;}.search-button__search-popup {display: none;position: fixed;top: 0;right: 0;left: 0;bottom: 0;z-index: 100;}.search-button__search-bg {position: absolute;top: 0;right: 0;left: 0;bottom: 0;background-color: rgba(0,0,0,0.5);}.search-button__search-form {display: block;width: 100%;position: relative;right: -100%;border: none;}.search-button__field-form {position: relative;z-index: 100;}.search-button__field {margin-top: 84px;outline: none;width: 100%;height: 55px;background-color: #a2a3a7;border: 2px solid #000;outline: none;font-size: 1.5em;text-align: center;}.search-button__field:-ms-input-placeholder {font-size: 1em;color: #000;}.search-button__field::placeholder {font-size: 1em;color: #000;}.social-list {display: -ms-flexbox;display: flex;-ms-flex-direction: row;flex-direction: row;-ms-flex-align: center;align-items: center;}.social-list__item {padding: 10px;}.social-list__item:hover {transform: scale(1.5);transition-duration: 0.5s;}.social-list__item:active {transform: scale(1.8);transition-duration: 0.3s;}.socials {display: none;}

